<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>    
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	
	<%
		//스크립트릿 - 1~10까지 합
		int sum = 0;
		for(int i = 1; i <= 10; i++) {
			sum = sum + i;
		}
	
	%>
	<%=sum %>
	
	
	<c:set var="sum" value="0" />
	<c:forEach var="i" begin="1" end="10" step="1" >
		<c:set var="sum" />
	</c:forEach>
	
	
	
	
	
	
</body>
</html>